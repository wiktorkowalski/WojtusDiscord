{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "Discord": "Host=${DB_HOST:-localhost};Port=${DB_PORT:-5432};Database=${DB_NAME:-discord_events};Username=${DB_USER:-postgres};Password=${DB_PASSWORD:-postgres}"
  },
  "Discord": {
    "Token": ""
  },
  "Database": {
    "AutoMigrate": true
  },
  "EventSettings": {
    "EnablePresenceEvents": true,
    "EnableTypingEvents": false
  }
}
